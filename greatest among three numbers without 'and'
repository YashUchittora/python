a=int(input("enter the first number: "))

b=int(input("enter the second number: "))

c=int(input("enter the third number: "))


if a>b:
    if a>c: 
     print("The greatest number is:",a)
    else:
        print("the greatest number is:",c)
elif b>c:
    print("The greatest number is:",b)  
else:
    print("the greatest number is:",c)
